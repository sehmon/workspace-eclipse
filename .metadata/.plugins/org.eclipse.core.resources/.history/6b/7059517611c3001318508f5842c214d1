package com.sehmon.whatamieating;

import java.util.ArrayList;

public class Additive {
	
	private String name;
	private ArrayList<String> additives;
	
	public Additive(String mName, ArrayList<String> list){
		this.setName(mName);
		this.setAdditives(list);
	}
	public void setName(String name) {
		this.name = name;
	}
	public ArrayList<String> getAdditives() {
		return additives;
	}
	public void setAdditives(ArrayList<String> additives) {
		this.additives = additives;
	}
	public String getName() {
		return name;
	}
	
	
}
