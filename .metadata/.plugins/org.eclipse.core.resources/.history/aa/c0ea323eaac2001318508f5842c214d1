public class Food {

	private String UPC;
	private String brand;
	
	public static Food fromJson(JSONObject jsonObject){
		
		Food f = new Food();
		try{
			
			f.brand = jsonObject.getString("upc");
			}			
		} catch(Exception e){
			e.printStackTrace();
			return null;
		}
		
		return b;
	}
}
